<% content_for :title, "Books" %>
<% if current_user && !current_user.librarian? %>
  <%= render 'member_dashboard', borrowings: @borrowings %>
  <%= render 'book_list' %>
<% end %>

<% if current_user && current_user.librarian? %>
  <%= render 'librarian_dashboard' %>
  <%= render 'borrowed_book_list' %>
<% end %>


